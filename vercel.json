{
	"rewrites": [
		{
			"source": "/admin/",
			"destination": "https://admin.web-app-frontend-lake.vercel.app/"
		},
		{
			"source": "/admin/:match*",
			"destination": "https://admin.web-app-frontend-lake.vercel.app/:match*"
		},
		{
			"source": "/",
			"destination": "https://web-app-frontend-lake.vercel.com/"
		},
		{
			"source": "/:match*",
			"destination": "https://web-app-frontend-lake.vercel.com/:match*"
		},
		{
			"source": "/driver",
			"destination": "https://driver.web-app-frontend-lake.vercel.com/"
		},
		{
			"source": "/driver/:match*",
			"destination": "https://driver.web-app-frontend-lake.vercel.com/:match*"
		}
	]
}
